{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "数据生产",
            "entry": "数据生产",
            "option": [
                "是否快速挂机"
            ]
        }
    ],
    "option": {
        "是否快速挂机": {
            "cases": [
                {
                    "name": "快速挂机1次",
                    "pipeline_override": {
                        "数据生产+快速挂机1次": {
                            "text": "文本1"
                        }
                    }
                },
                {
                    "name": "不快速挂机",
                    "pipeline_override": {
                        "数据生产+不快速挂机": {
                            "text": "文本2"
                        }
                    }
                }
            ]
        }
    }
}
